   "source": [
    "def fibonacci(num):\n",
    "    if num > 0:\n",
    "      print(0)\n",
    "      yield 0\n",
    "    if num > 1:\n",
    "      print(1)\n",
    "      yield(1)\n",
    "    a, b = 1, 1\n",
    "    for _ in range(num-2):\n",
    "      a, b = b, a+b\n",
    "      print(a)\n",
    "      yield(a)\n",
    "\n",
    "all((f < 1000 for f in fibonacci(100)))"
   ]
